<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Cliente.
    -->
    <changeSet id="20201009214106-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cliente_id"
                                 baseTableName="cliente_entregadores"
                                 constraintName="fk_cliente_entregadores_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cliente"/>

        <addForeignKeyConstraint baseColumnNames="entregadores_id"
                                 baseTableName="cliente_entregadores"
                                 constraintName="fk_cliente_entregadores_entregadores_id"
                                 referencedColumnNames="id"
                                 referencedTableName="entregador"/>
        
    </changeSet>
</databaseChangeLog>
